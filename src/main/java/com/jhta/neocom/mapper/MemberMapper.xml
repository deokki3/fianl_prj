<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jhta.neocom.mapper.MemberMapper">
	<insert id="insert" parameterType="com.jhta.neocom.model.MemberVo">
		insert into member_Info(id,password,nickname,name,phone,birth_date,reg_date,mem_no)
		values (#{id},#{password},#{nickname},#{name},#{phone},#{birth_date},now(),#{mem_no})
	</insert>

</mapper>